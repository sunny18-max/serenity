{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev", 
  "installCommand": "npm install --legacy-peer-deps && npm i @esbuild/linux-x64@latest @rollup/rollup-linux-x64-gnu@latest --no-save || true",
  "framework": "vite",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}